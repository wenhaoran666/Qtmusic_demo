}

QScrollBar:vertical {
    background-color: #f0f3f6; /* 滚动条背景 */
    width: 9px; /* 滚动条宽度 */
    margin: 0px 0px 0px 0px; /* 控制滚动条的间距 */
    border-radius: 4px;
}

QScrollBar::handle:vertical {
    background-color: #dcdfe4; /* 滚动条滑块颜色 */
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background-color: #bec3ca; /* 滚动条滑块悬停时颜色 */
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background-color: #f0f3f6; /* 隐藏滚动条上下箭头 */
}
QScrollBar:horizontal {
    height: 0px; /* 关键：将高度设为 0，使水平滚动条不可见且不占空间 */
    background-color: transparent; /* 可选：背景透明 */
}
